<!DOCTYPE html>
<html>
  <head>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <% yield :app_title %>
    <% yield :head %>
  </head>

  <body>
    <% yield :content %>
    <%= render template: 'layouts/govuk_admin_template' %>
